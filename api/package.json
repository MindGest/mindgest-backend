{
    "name": "mindgest-backend",
    "version": "0.1.0",
    "description": "Rest API for the MindGest Web Application",
    "main": "src/main.ts",
    "scripts": {
        "build": "tsc --project ./",
        "uninstall": "rm -rf build/ node_modules",
        "clean": "tsc --build --clean",
        "start": "node build/src/main.js",
        "dev": "tsc-watch --noClear --onSuccess \"npm start\" ",
        "test": "jest",
        "format": "prettier --write .",
        "format:check": "prettier --check ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MindGest/backend.git"
    },
    "keywords": [
        "MindGest",
        "WebApp",
        "Backend"
    ],
    "author": "MindGest",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/MindGest/backend/issues"
    },
    "homepage": "https://github.com/MindGest/backend#readme",
    "dependencies": {
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.14",
        "@types/jest": "^29.1.2",
        "@types/node": "^18.8.5",
        "@types/supertest": "^2.0.12",
        "jest": "^29.1.2",
        "prettier": "2.7.1",
        "prisma": "^4.4.0",
        "supertest": "^6.3.0",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.8.4"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "collectCoverage": true,
        "coverageReporters": [
            "text"
        ],
        "modulePathIgnorePatterns": [
            "./build/"
        ]
    }
}
